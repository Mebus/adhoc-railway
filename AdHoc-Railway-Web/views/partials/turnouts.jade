div.row
  div.span12
    h3 Turnouts

div.row
  div.span12
    a(href='addTurnout')
      i.icon-plus
div.row
  div.span12
    div.well.turnout(ng-repeat='turnout in turnouts')
      div.turnout.number
        {{turnout.number}}
      div.turnout.type
        {{turnout.type}}
      br
      div.turnout.editRemove
        a(href='editTurnout/{{turnout._id}}')
          i.icon-edit
        span(ng-click='deleteTurnout(turnout._id)')
          i.icon-remove


